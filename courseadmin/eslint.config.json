{
    "parserOptions":{
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion":2017
    },
    "env":{
        "browser":true,
        "node": true,
        "jquery": true
    },
    "rules":{
        "no-undef":2,
        "quotes":0,
        "no-trailing-spaces":0,
        "eol-last":0,
        "no-unused-vars":0,
        "no-underscore-dangle":0,
        "no-alert":0,
        "no-lone-blocks":0
    },
    "globals":{
        "jQuery": true,
        "$": true
    }
}